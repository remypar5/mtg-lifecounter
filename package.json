{
  "name": "mtg-lifecounter",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "jest-expo": "~19.0.0",
    "react-native-scripts": "1.1.0",
    "react-test-renderer": "16.0.0-alpha.12"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "node node_modules/jest/bin/jest.js",
    "test:dev": "node node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node node_modules/jest/bin/jest.js --coverage",
    "test:dev:coverage": "node node_modules/jest/bin/jest.js --watch --coverage"
  },
  "jest": {
    "preset": "jest-expo",
    "verbose": true,
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/?!(react-router-native|react-router)"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    }
  },
  "dependencies": {
    "expo": "^19.0.0",
    "prop-types": "^15.5.10",
    "react": "16.0.0-alpha.12",
    "react-native": "^0.46.1"
  }
}
