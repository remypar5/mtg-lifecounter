{
  "name": "mtg-lifecounter",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-register": "^6.26.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "jest-enzyme": "^4.0.1",
    "jest-expo": "^23.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.1.1",
    "react-native-scripts": "^1.7.0"
  },
  "scripts": {
    "start": "react-native start",
    "bundle": "react-native bundle --platform android --dev true --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "node node_modules/jest/bin/jest.js",
    "test:dev": "node node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node node_modules/jest/bin/jest.js --coverage",
    "test:dev:coverage": "node node_modules/jest/bin/jest.js --watch --coverage"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation)"
    ],
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    }
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "16.0.0-beta.5",
    "react-native": "^0.49.5",
    "react-navigation": "^1.0.0-beta.19"
  }
}
